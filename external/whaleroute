include(FetchContent)
Set(FETCHCONTENT_QUIET FALSE)
if (NOT TARGET whaleroute)
    FetchContent_Declare(
            whaleroute
            GIT_REPOSITORY https://github.com/kamchatka-volcano/whaleroute.git
            GIT_TAG        v1.0.0
            GIT_SHALLOW    ON
            GIT_PROGRESS TRUE
    )
    FetchContent_MakeAvailable(whaleroute)
endif()